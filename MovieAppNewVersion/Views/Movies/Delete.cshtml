@model MovieDeleteDTO
<h2>
    <b>@Model.MovieId</b> Are you sure do you want to delete this?
</h2>
<form asp-controller="Movies" asp-action="Delete" method="post">
    <input type="hidden" asp-for="MovieId" />
    <button type="submit" class="btn btn-primary">Delete</button>
    <a asp-action="List" asp-controller="Movies" asp-route-id="@Model.MovieId" class="btn btn-danger"> Cancel </a>
</form>