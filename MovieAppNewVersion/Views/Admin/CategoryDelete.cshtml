@model Category
<h2>
    <b>@Model.CategoryId</b> Are you sure do you want to delete this?
</h2>
<form asp-controller="Admin" asp-action="CategoryDelete" method="post">
    <input type="hidden" asp-for="CategoryId" />
    <button type="submit" class="btn btn-primary">Delete</button>
    <a asp-action="CategoryList" asp-controller="Admin" asp-route-id="@Model.CategoryId" class="btn btn-danger"> Cancel </a>
</form>
